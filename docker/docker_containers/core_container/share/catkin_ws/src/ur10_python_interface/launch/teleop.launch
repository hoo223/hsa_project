<?xml version="1.0"?>
<launch>
  <arg name="robot_name" default=""/>
  <arg name="unity" default="false"/>
  <arg name="gym_env" default="false"/>
  <arg name="rsa" default="false"/>
  <arg name="rand_agent" default="false"/>

  
  <param name="unity" value="$(arg unity)" type="bool"/>
  <param name="gym_env" value="$(arg gym_env)" type="bool"/>
  <param name="rsa" value="$(arg rsa)" type="bool"/>
  <param name="rand_agent" value="$(arg rand_agent)" type="bool"/>

  <node name="ik_solver" pkg="ur10_python_interface" type="ik_publisher" args="$(arg robot_name)" output="log"/>
  <node name="arm_state_publisher" pkg="ur10_python_interface" type="arm_state_publisher" args="$(arg robot_name)" output="log"/>
  <node name="joy2target_converter" pkg="ur10_python_interface" type="joy2target.py" args="$(arg robot_name)" output="screen"/>
  <node name="teleop_controller" pkg="ur10_python_interface" type="teleop_controller.py" args="$(arg robot_name)" output="screen"/> 

  <!-- <group if="$(arg gym_env)">
    <node name="rl_interface" pkg="ur10_python_interface" type="rl_interface.py" args="$(arg robot_name)" output="screen"/> 
  </group> -->
</launch>